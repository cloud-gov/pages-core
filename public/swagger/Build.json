{
  "type": "object",
  "required": [
    "id",
    "createdAt",
    "site",
    "state",
    "updatedAt",
    "user"
  ],
  "not": {
    "required": ["token"]
  },
  "properties": {
    "id": {
      "type": "integer"
    },
    "branch": {
      "type": ["string", "null"]
    },
    "commitSha": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "completedAt": {
      "type": "string"
    },
    "error": {
      "type": "string"
    },
    "source": {
      "type": "object",
      "required": [
        "owner",
        "repository"
      ],
      "properties": {
        "owner": {
          "type": "string"
        },
        "repository": {
          "type": "string"
        }
      }
    },
    "startedAt": {
      "type": "string"
    },
    "site": {
      "type": "object"
    },
    "state": {
      "type": "string",
      "enum": [
        "error",
        "processing",
        "skipped",
        "success",
        "queued",
        "created"
      ]
    },
    "updatedAt": {
      "type": "string"
    },
    "viewLink": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "not": {
        "required": ["githubAccessToken", "githubUserId"]
      }
    }
  }
}
